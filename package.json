    "test:e2e": "is-ci \test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test http://localhost:3000 cy:open",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "start:server": "nodemon server/server.js",
    "start:next": "next dev",
    "validate": "npm run test:coverage && npm run test:e2e:run"
  },
  "dependencies": {
    "@types/cypress": "^1.1.3",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cypress": "^12.17.1",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "express": "^4.18.2",
    "lowdb": "^6.0.1",
    "next": "13.4.10",
    "nodemon": "^3.0.1",
    "pg": "^8.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^9.1.3",
    "recoil": "^0.7.7",
    "sass": "^1.63.6",
