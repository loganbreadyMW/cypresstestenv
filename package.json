    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.26",
    "start-server-and-test": "^2.0.0"
